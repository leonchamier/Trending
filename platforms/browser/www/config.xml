<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mobile.trending" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Trending</name>
    <description>Trending app for events</description>
    <author email="onielhcharles@gmail.com" href="http://Onieevents.com">Oniel Charles</author>
    <content src="index.html" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="cli-8.1.1" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyAQb8UXtwwrW-R3zzOWgEDqvMNuA6IVkxs" />
    <preference name="GOOGLE_MAPS_IOS_API_KEY" value="AIzaSyAQb8UXtwwrW-R3zzOWgEDqvMNuA6IVkxs" />
</widget>
